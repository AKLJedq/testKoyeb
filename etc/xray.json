{
	"inbounds": [{
		"port": 443,
		"protocol": "vless",
		"settings": {
			"clients": [{
				"id": "$AUUID"
			}],
		"decryption": "none"
		},
		"streamSettings": {
			"network": "tcp",
			"security": "tls",
			"tlsSettings": {
				"serverName": "viber.com",
				"certificates": [{
					"usage": "issue",
					"cert_type": "self",
					"certificateFile": "/etc/v2ray.ca.crt",
					"keyFile": "/etc/v2ray.ca.key"
				}]
			}
		}
	}],
	"outbounds": [{
		"protocol": "freedom"
	}]

}
