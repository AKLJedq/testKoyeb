{
   "inbounds":[
      {
         "listen":"/etc/caddy/vmess",
         "protocol":"vmess",
         "settings":{
            "clients":[
               {
                  "id":"$AUUID"
               }
            ]
         },
         "streamSettings":{
            "network":"tcp",
            "security":"tls",
            "tlsSettings":{
               "serverName":"viber.com",
               "certificates":[
                  {
                     "usage":"issue",
                     "cert_type":"self",
                     "certificateFile":"/etc/v2ray.ca.crt",
                     "keyFile":"/etc/v2ray.ca.key"
                  }
               ]
            }
         }
      },
      {
         "listen":"/etc/caddy/vless",
         "protocol":"vless",
         "settings":{
            "clients":[
               {
                  "id":"$AUUID"
               }
            ],
            "decryption":"none"
         },
         "streamSettings":{
            "network":"ws",
            "wsSettings":{
               "path":"/$AUUID-vless"
            }
         }
      },
      {
         "listen":"/etc/caddy/trojan",
         "protocol":"trojan",
         "settings":{
            "clients":[
               {
                  "password":"$AUUID"
               }
            ]
         },
         "streamSettings":{
            "network":"ws",
            "wsSettings":{
               "path":"/$AUUID-trojan"
            }
         }
      }
   ],
   "outbounds":[
      {
         "protocol":"freedom"
      }
   ]
}
